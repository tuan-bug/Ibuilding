@model IEnumerable<IBUIDING.Models.Project>

@{
    ViewBag.Title = "Projects";
}

<h2>Projects</h2>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Code Project</th>
            <th>Name Project</th>
            <th>Phone Contact</th>
            <th>Address Project</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.CodeProject)</td>
                <td>@Html.DisplayFor(modelItem => item.NameProject)</td>
                <td>@Html.DisplayFor(modelItem => item.PhoneContact)</td>
                <td>@Html.DisplayFor(modelItem => item.AddressProject)</td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
